{
  "title": "Panduan Implementasi",
  "description": "Dokumentasi GitBook",
  "plugins": [
    "highlight",
    "search",
    "lunr",
    "sharing",
    "fontsettings",
    "theme-default",
    "youtube",
    "flexible-embed"
  ],
  "pluginsConfig": {
    "sharing": {
      "facebook": true,
      "twitter": true
    },
    "youtube": {
      "embed": true
    },
    "flexible-embed": {
      "youtube": {
        "width": "100%",
        "height": "480px"
      },
      "default": {
        "align": "center"
      }
    }
  }
}
