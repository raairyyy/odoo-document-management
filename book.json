{
  "title": "Panduan Implementasi",
  "description": "Dokumentasi GitBook",
  "plugins": [
    "highlight",
    "search",
    "lunr",
    "sharing",
    "fontsettings",
    "theme-default",
    "youtube",
    "advanced-embed",
    "embed"
  ],
  "pluginsConfig": {
    "sharing": {
      "facebook": true,
      "twitter": true
    },
    "youtube": {
      "embed": true
    },
    "advanced-embed": {
      "height": "480",
      "width": "100%",
      "align": "center"
    },
    "embed": {
      "url": true,
      "code": true
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 2
    }
  }
}
