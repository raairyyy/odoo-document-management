{
  "title": "My Book",
  "plugins": [
    "youtube",
    "search",
    "lunr",
    "highlight",
    "sharing",
    "fontsettings",
    "theme-default"
  ]
}

  "pluginsConfig": {
    "embed": {
      "target": "_blank",
      "code": true
    },
    "richmedia": {
      "siteDomains": ["youtube.com", "youtu.be", "vimeo.com"]
    },
    "theme-default": {
      "showLevel": true
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": ["facebook", "twitter"]
    }
  }
}
