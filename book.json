{
  "title": "Panduan Implementasi",
  "description": "Dokumentasi GitBook untuk panduan implementasi sistem.",
  "plugins": [
    "highlight",
    "search",
    "lunr",
    "sharing",
    "fontsettings",
    "theme-default",
    "youtube",
    "embed",
    "richmedia"
  ],
  "pluginsConfig": {
    "embed": {
      "target": "_blank",
      "code": true
    },
    "richmedia": {
      "siteDomains": ["youtube.com", "youtu.be", "vimeo.com"]
    },
    "theme-default": {
      "showLevel": true
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": ["facebook", "twitter"]
    }
  }
}
